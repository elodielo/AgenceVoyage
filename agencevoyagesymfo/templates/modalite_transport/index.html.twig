{% extends 'base.html.twig' %}

{% block title %}ModaliteTransport index{% endblock %}

{% block body %}
    <h1>ModaliteTransport index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for modalite_transport in modalite_transports %}
            <tr>
                <td>{{ modalite_transport.id }}</td>
                <td>{{ modalite_transport.nom }}</td>
                <td>
                    <a href="{{ path('app_modalite_transport_show', {'id': modalite_transport.id}) }}">show</a>
                    <a href="{{ path('app_modalite_transport_edit', {'id': modalite_transport.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_modalite_transport_new') }}">Create new</a>
{% endblock %}
